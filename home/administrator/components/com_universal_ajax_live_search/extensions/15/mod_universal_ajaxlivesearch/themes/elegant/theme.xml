  <params>
    <param name="skin" theme="elegant" type="skin" default="" size="5" label="Skin" description="" />

		<param name="@spacer" type="spacer" default="" label="" description="" />
    <param name="ajaxloaderimage" type="imageradio" default="fadinglines.gif" label="Animation for loading" description="" directory="/images/loaders" exclude="" stripext="" />
    <param name="closeimage" type="imageradio" default="close_rounded.png" label="Image for close-button" description="" directory="/images/close" exclude="" stripext="" />
    <param name="closeimagecolor" type="color" default="000000" size="7" label="Close image color" description="" />
		
    <param name="@spacer" type="spacer" default="" label="" description="" />
    <param name="searchbuttongradient" type="gradient" default="47c5ed-188dd9" size="15" label="The search button background gradient color" description="" />
    <param name="searchbuttonimage" type="imageradio" default="magnifier_strong_mid.png" label="Image for search button" description="" directory="/images/search_button" exclude="" stripext="" />
		
    <param name="@spacer" type="spacer" default="" label="" description="" />
    <param name="plugintitlegradient" type="gradient" default="E4EAEE-E4EAEE" size="15" label="Plugin title background gradient color" description="" />
    <param name="plugintitlefontcolor" type="color" default="4E6170" size="15" label="Plugin title font color" description="" />
    <param name="actualpaginatorimage" type="imageradio" default="pager_blue.png" label="Image for actual paginator state" description="" directory="/images/paginators" exclude="" stripext="" />
    <param name="hoverpaginatorimage" type="imageradio" default="pager-hover.png" label="Image for hover paginator state" description="" directory="/images/paginators" exclude="" stripext="" />
    <param name="inactivepaginatorimage" type="imageradio" default="pager.png" label="Image for inactive paginator state" description="" directory="/images/paginators" exclude="" stripext="" />

		<param name="@spacer" type="spacer" default="" label="" description="" />
    <param name="resultcolor" type="color" default="FFFFFF" size="15" label="Result-element background color" description="" />
    <param name="resultfontcolor" type="color" default="4E6170" size="15" label="Result-element font color" description="" />
    <param name="resultintrotextcolor" type="color" default="7794aa" size="15" label="Result-element intro-text font color" description="" />
    <param name="resultbordertopcolor" type="color" default="e5e5e5" size="15" label="Result-element border-top color" description="" />
    <param name="resultborderbottomcolor" type="color" default="c2d5e1" size="15" label="Result-element border-bottom color" description="" />
    
		<param name="@spacer" type="spacer" default="" label="" description="" />
    <param name="activeresultgradient" type="gradient" default="188dd9-18517d" size="15" label="Hover, Active result-element background gradient color" description="" />
    <param name="activeresultfontcolor" type="color" default="FFFFFF" size="15" label="Hover, Active result-element font color" description="" />

    <param name="boxshadow" type="radio" default="1" label="Box shadow" description="">
		  <option value="1">Yes</option>
		  <option value="0">No</option>
		</param>
		
		<param name="rounded" type="radio" default="1" label="Rounded Corners" description="">
		  <option value="1">Yes</option>
		  <option value="0">No</option>
		</param>
  </params>
</theme>
